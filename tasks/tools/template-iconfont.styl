@font-face {
    font-family: "<%= fontName %>";
    src: url('<%= fontPath %><%= fontName %>-<%= version %>/<%= fontName %>.eot');
    src: url('<%= fontPath %><%= fontName %>-<%= version %>/<%= fontName %>.eot?#iefix') format('eot'),
        url('<%= fontPath %><%= fontName %>-<%= version %>/<%= fontName %>.woff') format('woff'),
        url('<%= fontPath %><%= fontName %>-<%= version %>/<%= fontName %>.ttf') format('truetype'),
        url('<%= fontPath %><%= fontName %>-<%= version %>/<%= fontName %>.svg#<%= fontName %>') format('svg');
    font-weight: normal;
    font-style: normal;
}

.<%= className %> {
    font-family: "<%= fontName %>";
    font-style: normal;
    font-weight: normal;
}

<% _.each(glyphs, function(glyph) { %>
$<%= className %>-<%= glyph.name %> = "\<%= glyph.codepoint.toString(16).toUpperCase() %>";<% }); %>

<% _.each(glyphs, function(glyph) { %>
.<%= className %>-<%= glyph.name %>:before { content: $<%= className %>-<%= glyph.name %> }
<% }); %>
